<h2> Trickles of Change Support </h2>

<%= form_for @email do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <p>
    <%= f.label :topic %><br />
    <%= f.text_field :topic %>
  </p>
  <p>
    <%= f.label :message %><br />
    <%= f.text_area :message %>
  </p>
  <p><%= f.submit %></p>
<% end %>


